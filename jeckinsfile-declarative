pipeline {
    agent any 
    stages {
        stage('SCM-checkout') { 
            steps {
                git branch: 'main', credentialsId: 'git-credentials', 
				        url: 'https://github.com/prowesspeace/main-webapp-my-new-program.git' 
            }
        }
        stage('compile') { 
            steps {
                sh "mvn compile" 
            }
        }
        stage('Test') { 
            steps {
                sh "mvn test" 
            }
        }
		stage('Package') { 
            steps {
                sh " mvn package"
            }
        }
    }
}
